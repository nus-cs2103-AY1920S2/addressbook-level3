@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Interface ReadOnlyTaTracker <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class TaTracker
Class ReadOnlyTaTracker
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Student {
Class Student
Class UniqueStudentList
}

Package Session {
Class Session
Class UniqueSessionList
Class UniqueDoneSessionList
}

Package Group {
Class Group
Class UniqueGroupList
}

Package Module {
Class Module
Class UniqueModuleList
}

}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

TaTracker .up.|> ReadOnlyTaTracker

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" TaTracker
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

TaTracker *--> "1" UniqueStudentList
UniqueStudentList o--> "*" Student
TaTracker *--> "1" UniqueModuleList
UniqueModuleList o--> "*" Module
TaTracker *--> "1" UniqueGroupList
UniqueGroupList o--> "*" Group
TaTracker *--> "1" UniqueSessionList
UniqueSessionList o-down-> "*" Session
TaTracker *--> "1" UniqueDoneSessionList
UniqueDoneSessionList o-down-> "*" Session

ModelManager -->"1" Student : filtered list
ModelManager -->"1" Module : filtered list
ModelManager -->"1" Group : filtered list
ModelManager -right->"1" Session : filtered list

@enduml
