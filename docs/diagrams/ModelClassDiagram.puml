@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>> {
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Interviewee {
Class Interviewee
Class Alias
Class ID
Class Name
Class CV
Class Transcript
Class IntervieweeList
}

Package Question {
Class Question
Class QuestionDescription
Class QuestionList
}

Package Attribute {
Class Attribute
Class AttributeName
Class AttributeList
}

Package Metric {
Class MetricList
Class Metric
Class MetricName
Class AttributeToWeightMap
}

Package Transcript {
Class Transcript
Class RemarkList
Class AttributeToScoreMap
}

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o-left-> "1" UserPrefs
