@startuml
!include style.puml

Actor User as user USER_COLOR
Participant ":EYLAH" as ui UI_COLOR
Participant ":LogicManager" as logic LOGIC_COLOR
Participant ":ModelManager" as model MODEL_COLOR

user -[USER_COLOR]> ui : "weight 73"
activate ui UI_COLOR

ui -[UI_COLOR]> logic : execute("weight 73")
activate logic LOGIC_COLOR

create "w:Weight" as weight STORAGE_COLOR
logic -[LOGIC_COLOR]> weight : new Weight(170)
activate weight STORAGE_COLOR

weight -[STORAGE_COLOR]-> logic
deactivate weight

logic -[LOGIC_COLOR]> model : setWeight(w)
activate model MODEL_COLOR

model -[MODEL_COLOR]-> logic
deactivate model

logic --[LOGIC_COLOR]> ui : CommandResult
deactivate logic

ui--[UI_COLOR]> user : CommandResult
deactivate ui
@enduml
