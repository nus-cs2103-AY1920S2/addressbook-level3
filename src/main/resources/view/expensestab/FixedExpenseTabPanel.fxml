<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<SplitPane fx:id="splitPane" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@PieChartColor.css"/>
    </stylesheets>
    <VBox fx:id="vbox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
        <ListView fx:id="fixedExpenseListView" VBox.vgrow="ALWAYS"/>
    </VBox>
    <PieChart fx:id="pieChart"/>
</SplitPane>
