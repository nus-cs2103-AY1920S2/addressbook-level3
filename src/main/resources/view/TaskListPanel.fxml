<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="tasksHeader" graphicTextGap="1.0" text="Tasks" textFill="#453fe9">
      <font>
         <Font size="24.0" />
      </font>
      <VBox.margin>
         <Insets bottom="20.0" top="20.0" />
      </VBox.margin>
   </Label>
    <ListView fx:id="taskListView" VBox.vgrow="ALWAYS" />
</VBox>
