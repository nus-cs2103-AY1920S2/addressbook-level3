<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="pane-with-border" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="itemDisplayList" stylesheets="@ItemDisplay.css" VBox.vgrow="ALWAYS">
         <padding>
            <Insets right="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </ListView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="0.0" top="10.0" />
   </padding>
</VBox>
