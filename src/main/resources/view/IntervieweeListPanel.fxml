<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.Pane?>

<VBox id="intervieweeListPanel" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <Pane minHeight="120" />
   <BorderPane VBox.vgrow="ALWAYS">
      <center>
         <ListView fx:id="intervieweeListView" prefHeight="350" prefWidth="330" />
      </center>
      <top>
            <Label fx:id="title" alignment="CENTER" prefHeight="20.0" prefWidth="900" text="\$title"/>
      </top>
   </BorderPane>
</VBox>
