<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<StackPane fx:id="calendarCardPanel" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox>
            <children>
                <StackPane alignment="TOP_LEFT" maxHeight="1.5" minHeight="1.5" prefHeight="1.5" style="-fx-background-color: #80d8ff;">
                    <VBox.margin>
                        <Insets left="10.0" right="20.0" top="5.0" />
                    </VBox.margin>
                </StackPane>
                <StackPane>
                    <children>
                        <VBox fx:id="cardHolder" spacing="2.0">
                            <StackPane.margin>
                                <Insets />
                            </StackPane.margin>
                        </VBox>
                    </children>
                    <VBox.margin>
                        <Insets left="10.0" right="20.0" top="1.0" />
                    </VBox.margin>
                </StackPane>
            </children>
        </VBox>
    </children>
</StackPane>
