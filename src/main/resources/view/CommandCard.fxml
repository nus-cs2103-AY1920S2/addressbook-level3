<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="commandPane" fx:id="cardPane" style="-fx-background-color: transparent;" HBox.hgrow="ALWAYS"
      xmlns="http://javafx.com/javafx/11.0" xmlns:fx="http://javafx.com/fxml/1">
  <stylesheets>
    <URL value="@CommandCard.css"/>
  </stylesheets>
  <children>
    <AnchorPane HBox.hgrow="ALWAYS">
      <children>
        <HBox minWidth="614.0" style="-fx-background-color: #46998F; -fx-background-radius: 10;"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0" HBox.hgrow="ALWAYS" minHeight="250">
          <children>
            <HBox style="-fx-border-color: white; -fx-border-width: 1; -fx-border-style: segments(4, 4, 4, 4); -fx-border-radius: 10; "
                  HBox.hgrow="ALWAYS">
              <children>
                <VBox alignment="CENTER_LEFT" minHeight="105" prefHeight="130.0" prefWidth="431.0" HBox.hgrow="ALWAYS">
                  <padding>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                  </padding>
                  <children>
                    <HBox alignment="CENTER_LEFT" prefWidth="500.0" spacing="10.0">
                      <children>
                        <HBox spacing="3.0">
                          <children>
                            <Label id="name" fx:id="name" text="name"/>
                          </children>
                        </HBox>
                      </children>
                    </HBox>
                    <VBox prefHeight="200.0" HBox.hgrow="ALWAYS">
                      <children>
                        <Label id="usage" fx:id="usage" text="usage" wrapText="true"/>
                      </children>
                    </VBox>
                  </children>
                </VBox>
              </children>
              <HBox.margin>
                <Insets bottom="14.0" left="14.0" right="14.0" top="14.0"/>
              </HBox.margin>
            </HBox>
          </children>
        </HBox>
        <ImageView fitHeight="19.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true"
                   AnchorPane.rightAnchor="7.5" AnchorPane.topAnchor="55.0">
          <image>
            <Image url="@../images/scissors.png"/>
          </image>
        </ImageView>
      </children>
    </AnchorPane>
  </children>

</HBox>
