<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<fx:root resizable="false" title="Statistics" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/flashspeed_logo.png" />
  </icons>
  <scene>
    <Scene>
         <HBox alignment="CENTER" maxWidth="800" maxHeight="800">
            <children>
               <Label fx:id="stats" text="Label" style="-fx-font-size: 20;" wrapText="true">
                  <HBox.margin>
                     <Insets right="5.0" />
                  </HBox.margin>
               </Label>

            </children>
            <opaqueInsets>
               <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
            </opaqueInsets>
            <padding>
               <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
            </padding>
         </HBox>
    </Scene>
  </scene>
</fx:root>
