<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>

<GridPane styleClass="module-details-panel" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" style = "-fx-background-color: #424659; -fx-background-radius: 15px; -fx-border-color: transparent; -fx-effect: null;">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
    </columnConstraints>
    <Label fx:id="moduleDetails" textOverrun="CLIP" wrapText="true" textAlignment="CENTER" style="-fx-text-fill: white; -fx-font-family: Arial; -fx-font-size: 14pt; -fx-font-weight: bold; -fx-padding: 3px;"/>
</GridPane>
