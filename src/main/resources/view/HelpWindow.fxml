<fx:root title="Help" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/help_icon.png" />
  </icons>
  <scene>
    <Scene>
        <AnchorPane>
            <WebView fx:id="guideView" prefHeight="450.0" prefWidth="700.0"
