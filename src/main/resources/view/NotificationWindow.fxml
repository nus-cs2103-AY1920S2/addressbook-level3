<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>

<fx:root minHeight="240.0" minWidth="240.0" resizable="false" title="Alert!" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
<icons>
    <Image url="@/images/info_icon.png" />
</icons>
    <scene>
    <Scene>
        <HBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="193.0" prefWidth="255.0" style="-fx-background-color: #101010;">
            <children>
               <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                     <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Low quantity notification" textAlignment="CENTER" textFill="#e797fd" wrapText="true">
                        <font>
                           <Font size="20.0" />
                        </font>
                     </Label>
                      <Label fx:id="notificationMessage" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="217.0" prefWidth="253.0" text="Label" textAlignment="CENTER" textFill="WHITE" wrapText="true" VBox.vgrow="ALWAYS" />
                  </children>
               </VBox>
            </children>
            <opaqueInsets>
                <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
            </opaqueInsets>
            <padding>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
            </padding>
        </HBox>
    </Scene>
    </scene>
</fx:root>
